{
  "multipart": [
    {
      "apply": {
        "model": "nyatec:block/cable/test/center"
      }
    },
    {
      "apply": {
        "model": "nyatec:block/cable/test/side",
        "uvlock": false,
        "y": 0
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "nyatec:block/cable/test/side",
        "uvlock": false,
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "nyatec:block/cable/test/side",
        "uvlock": false,
        "y": 180
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "nyatec:block/cable/test/side",
        "uvlock": false,
        "y": 270
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "nyatec:block/cable/test/top_bottom",
        "uvlock": false,
        "x": 180
      },
      "when": {
        "up": "true"
      }
    },
    {
      "apply": {
        "model": "nyatec:block/cable/test/top_bottom",
        "uvlock": false,
        "x": 0
      },
      "when": {
        "down": "true"
      }
    }
  ]
}